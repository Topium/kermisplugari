{
    "manifest_version": 3,
    "name": "Kermisplugari",
    "description": "Lets kermis",
    "version": "1.0",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },
    "content_scripts": [
        {
            "js": ["scripts/kermis.js"],
            "matches": [
            "https://yle.fi/*"
            ]
        }
    ],
    "web_accessible_resources": [
        {
        "resources": [ "images/kermis1.jpg" ],
        "matches": [ "https://yle.fi/*" ]
        }
    ]
  }